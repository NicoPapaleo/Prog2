package Series;

public class Serie {
	
	//Atributos
	private String titulo;
	private String descripcion;
	private String creador;
	private String genero;
	private Temporada[] temporadas;

	public Serie(int cantidadTemporadas, String titulo, String descripcion, String creador, String genero) {
		temporadas = new Temporada[cantidadTemporadas];
		this.titulo = titulo;
		this.descripcion = descripcion;
		this.creador = creador;
		this.genero = genero;
	}
	
	//Metodos
	
	public String getDescripcion() {
		return descripcion;
	}
	
	public String getCreador() {
		return creador;
	}
	
	public String getGenero() {
		return genero;
	}
	
	public String getTitulo() {
		return titulo;
	}
	
	//public int getTemporada() {
	//	return temporadas;
	//}
	
	public void getDatosCompletos() {
		System.out.println("Nombre: " + this.titulo +
							//"\nTemporada: " + this.temporada + 
							"\nGenero: " + this.genero + 
							"\nDescripcion: " + this.descripcion +
							"\nCreador: " + this.creador);
	}
}
