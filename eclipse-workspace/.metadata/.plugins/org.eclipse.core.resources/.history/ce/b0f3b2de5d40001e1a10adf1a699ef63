package Electrodomesticos;

public class Electrodomestico {
	
	//-----Atributos--------
	private String nombre;
	private double precio;
	private String color;
	private double consumoEnergetico;
	private double peso;
	
	//-------Constructores-------
	
	public Electrodomestico() {
		color = "Gris plata";
		precio = 100.00;
		consumoEnergetico = 10.00;
		peso = 2.00;
	}
	

}
