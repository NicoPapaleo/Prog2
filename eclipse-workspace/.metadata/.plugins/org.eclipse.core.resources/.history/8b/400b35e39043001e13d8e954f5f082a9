package AgendaPersonal;

public class Persona {
	
	private int tel;
	private String email;
	
	public Persona(int tel, String email) {
		this.tel = tel;
		this.email = email;
	}

	public int getTel() {
		return tel;
	}

	public void setTel(int tel) {
		this.tel = tel;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}
	

}
