package Electrodomesticos;

public class Electrodomestico {
	
	//-----Atributos--------
	private String nombre;
	private double precio;
	private String color;
	private double consumoEnergetico;
	private double peso;
	
	//-------Constructores-------
	
	public Electrodomestico() {
		color = "Gris plata";
		precio = 100.00;
		consumoEnergetico = 10.00;
		peso = 2.00;
	}
	
	public Electrodomestico(String nombre) {
		this.nombre = nombre;
	}
	
	public Electrodomestico(String nombre, double precio) {
		this.nombre = nombre;
		this.precio = precio;
	}
	
	public Electrodomestico(String nombre, double precio, String color) {
		this.nombre = nombre;
		this.precio = precio;
		this.color = color;
	}
	
	public Electrodomestico(String nombre, double precio, String color, double peso) {
		this.nombre = nombre;
		this.precio = precio;
		this.color = color;
		this.peso= peso;
	}

	
	
}
