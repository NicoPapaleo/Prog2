package Persona;

import java.time.LocalDate;

public class Main {
	public static void main(String[] args) {
		Persona persona1 = new Persona("Nicolas", "Papaleo", 41873041);
		persona1.setPeso(70.5);
		persona1.setAltura(1.77);
		persona1.setEdad(24);
		persona1.setFechaDeNacimiento(LocalDate.of(1999, 07, 02));
		

	}

}
